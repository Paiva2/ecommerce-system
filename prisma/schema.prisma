generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @default(uuid())
  email      String    @unique @db.VarChar(50)
  username   String?   @db.VarChar(50)
  password   String?   @db.VarChar(100)
  created_At DateTime? @default(now())

  @@map("user")
}
